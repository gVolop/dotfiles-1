[core]
	editor = vim
	excludesfile = /Users/pecoyamada/.gitignore_global
[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  type = cat-file -t
  dump = cat-file -p
	d = difftool
[diff]
	tool = vimdiff
[difftool]
	prompt = false
[push]
	default = simple
[merge]
	tool = vimdiff
